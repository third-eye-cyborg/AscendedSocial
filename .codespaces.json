{
  "repositories": {
    "third-eye-cyborg/AscendedSocial": {
      "devcontainerPath": ".devcontainer/devcontainer.json",
      "defaultLanguage": "typescript",
      "extensions": [
        "GitHub.copilot",
        "GitHub.copilot-chat"
      ]
    }
  },
  "customization": {
    "openFiles": [
      "README.md",
      ".devcontainer/ONBOARDING.md",
      "client/src/App.tsx"
    ],
    "onCreateCommand": ".devcontainer/init.sh",
    "postCreateCommand": "npm install && npm run check",
    "postStartCommand": "npm run dev"
  },
  "prebuilds": {
    "enabled": true,
    "template": "default",
    "retention": "one_week"
  },
  "debugInfo": true
}

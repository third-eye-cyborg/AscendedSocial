  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run storybook\"",
    "dev:server": "PORT=8080 NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite --port 3000 --host",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:visual:all": "npm run test:storybook && npm run test:cypress && npm run test:playwright",
    "test:storybook": "test-storybook --url http://localhost:6006",
    "test:cypress": "cypress run --component",
    "test:playwright": "playwright test",
    "chromatic:storybook": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "chromatic:cypress": "chromatic --cypress --project-token=$CHROMATIC_PROJECT_TOKEN",
    "chromatic:playwright": "chromatic --playwright --project-token=$CHROMATIC_PROJECT_TOKEN",
    "mcp:start": "node scripts/start-mcp-servers.js",
    "mcp:logs": "tail -f logs/mcp-servers.log",
    "logs:all": "concurrently \"npm run mcp:logs\" \"tail -f logs/visual-tests.log\" \"tail -f logs/replit.log\"",
    "builder:dev": "npm run dev",
    "builder:dev:client": "npm run dev:client",
    "builder:dev:server": "npm run dev:server",
    "builder:storybook": "npm run storybook",
    "builder:generate": "builder generate --framework react --output ./client/components",
    "builder:sync": "builder sync --design-system --figma",
    "builder:pull": "builder pull --models --components",
    "builder:push": "builder push --components --design-tokens",
    "builder:preview": "builder preview --port 3001",
    "builder:deploy": "builder deploy --platform replit",
    "builder:figma:sync": "builder figma sync --file-key $FIGMA_FILE_KEY --access-token $FIGMA_ACCESS_TOKEN",
    "builder:ai:generate": "builder ai generate --context social-media --openai-key $OPENAI_API_KEY"
  },
